Tavaho DATABASE
    users TABLE
        - userID varchar(60) PRIMARY_KEY
        - userName varchar(70)
        - userEmail varchar(50)
        - userPassword text
        - userProfilePic varchar(200)
        - userDescription

    jobs TABLE
        - jobID varchar(60) PRIMARY_KEY
        - userID varchar(70) FOREIGN_KEY
        - jobTitle varchar(255)
        - jobBody text
        - jobLanguage varchar(355)
        - jobRequirements text
        - jobContactNum varchar(20)
        - jobLocation varchar(355)
        - jobDatePosted datetime
        - jobSalaryFrom varchar(355)
        - jobSalaryTo varchar(355)
